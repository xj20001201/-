#include <stdio.h>
int main()
{
	int num, i, j, k;
	int a = 0;
	for (i = 1; i <= 4; i++)
	{
		for (j = 1; j <= 4; j++)
		{
			for (k = 1; k <= 4; k++)
			{
				if (i != j && i != k && j != k)
				{
					a = a + 1;
					num = i * 100 + j * 10 + k;
					printf("%d  ", num);
				}
			}
		}
	}
	printf("共有%d个数字", a);

	return 0;
